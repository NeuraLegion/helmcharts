{
  "title": "ao-firebase",
  "steps": [
    {
      "type": "setViewport",
      "width": 922,
      "height": 1007,
      "deviceScaleFactor": 1,
      "isMobile": false,
      "hasTouch": false,
      "isLandscape": false
    },
    {
      "type": "navigate",
      "url": "http://localhost:8080/login",
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "http://localhost:8080/login",
          "title": "Sign-in"
        }
      ]
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [["aria/Email"], ["#username"]],
      "offsetY": 40.703125,
      "offsetX": 6
    },
    {
      "type": "change",
      "value": "root@brightsec.com",
      "selectors": [["aria/Email"], ["#username"]],
      "target": "main"
    },
    {
      "type": "keyDown",
      "target": "main",
      "key": "Tab"
    },
    {
      "type": "keyUp",
      "key": "Tab",
      "target": "main"
    },
    {
      "type": "change",
      "value": "root@brightsec.com",
      "selectors": [["aria/Password"], ["#password"]],
      "target": "main"
    },
    {
      "type": "keyDown",
      "target": "main",
      "key": "Enter"
    },
    {
      "type": "keyUp",
      "key": "Enter",
      "target": "main"
    }
  ]
}
